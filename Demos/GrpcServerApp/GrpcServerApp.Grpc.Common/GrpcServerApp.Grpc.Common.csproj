<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Grpc.AspNetCore" Version="2.71.0" />
  </ItemGroup>

	<ItemGroup>
		<Protobuf Include="Protos\common.proto" GrpcServices="Server" />
		<Protobuf Include="Protos\reply_data.proto" GrpcServices="Server" />
		<Protobuf Include="Protos\request_data.proto" GrpcServices="Server" />
		<Protobuf Include="Protos\business_transaction_description.proto" GrpcServices="Server" />
	</ItemGroup>

</Project>
